{
  "label": "$SERVICE_NAME Deployment Policy",
  "description": "CPU Telemetry Open Horizon Deployment Policy",
  "service": {
    "name": "$SERVICE_NAME",
    "org": "$HZN_ORG_ID",
    "arch": "amd64",
    "serviceVersions": [
      {
        "version": "$SERVICE_VERSION",
        "priority":{}
      }
    ]
  },
  "properties": [],
  "constraints": [
	  "think == $DOCKERHUB_ID"
  ],
  "userInput": [
    {
      "serviceOrgid": "$HZN_ORG_ID",
      "serviceUrl": "$SERVICE_NAME",
      "serviceVersionRange": "[$SERVICE_VERSION]",
      "inputs": [
        {
          "name": "MQTT_ORGID",
          "label": "Watson IoT Platform Org",
          "type": "string",
          "value": "5yrusp"
        },
        {
          "name": "MQTT_DEVICETYPE",
          "label": "Watson IoT DeviceType",
          "type": "string",
          "value": "ThinkEdge"
        },
        {
          "name": "MQTT_DEVICEID",
          "label": "Watson IoT Device",
          "type": "string",
          "value": "instructor"
        },
        {
          "name": "MQTT_DEVICETOKEN",
          "label": "Watson IoT Device Token",
          "type": "string",
          "value": "Think2022"
        }
      ]
    }
  ]
}
